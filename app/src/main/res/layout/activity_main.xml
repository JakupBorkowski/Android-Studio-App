<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/btn_gyroscope9"
        android:layout_width="126dp"
        android:layout_height="77dp"
        android:onClick="CheckSamplesActivity"
        android:text="Get samples"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.221"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope6"
        app:layout_constraintVertical_bias="0.113" />

    <EditText
        android:id="@+id/et_okres"
        android:layout_width="167dp"
        android:layout_height="42dp"
        android:layout_marginTop="132dp"
        android:layout_marginEnd="32dp"
        android:ems="10"
        android:hint="okres [ms]"
        android:inputType="number"
        android:text="100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_gyroscope8"
        android:layout_width="130dp"
        android:layout_height="63dp"
        android:layout_marginEnd="44dp"
        android:onClick="DeleteSessionActivity"
        android:text="Delete Session"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.854"
        app:layout_constraintStart_toEndOf="@+id/btn_gyroscope4"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope7"
        app:layout_constraintVertical_bias="0.042" />

    <Button
        android:id="@+id/btn_gyroscope7"
        android:layout_width="124dp"
        android:layout_height="64dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="44dp"
        android:onClick="DeleteSensorActivity"
        android:text="Delete Sensor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toEndOf="@+id/btn_gyroscope3"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope5" />

    <Button
        android:id="@+id/btn_gyroscope5"
        android:layout_width="126dp"
        android:layout_height="60dp"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="56dp"
        android:onClick="DeleteDeviceActivity"
        android:text="Delete Device"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_okres" />

    <Button
        android:id="@+id/btn_gyroscope4"
        android:layout_width="126dp"
        android:layout_height="77dp"
        android:onClick="AddSessionActivity"
        android:text="Add Session"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope3" />

    <Button
        android:id="@+id/btn_gyroscope6"
        android:layout_width="126dp"
        android:layout_height="77dp"
        android:layout_marginTop="12dp"
        android:onClick="SessionHasSensorActivity"
        android:text="Add SessionHasSensor"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.221"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope4" />

    <Button
        android:id="@+id/btn_gyroscope2"
        android:layout_width="125dp"
        android:layout_height="57dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="12dp"
        android:onClick="NewDeviceActivity"
        android:text="Add Device"
        app:layout_constraintEnd_toStartOf="@+id/btn_gyroscope5"
        app:layout_constraintHorizontal_bias="0.09"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope" />

    <Button
        android:id="@+id/btn_gyroscope3"
        android:layout_width="126dp"
        android:layout_height="64dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="20dp"
        android:onClick="AddSensorActivity"
        android:text="Add Sensor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_gyroscope2" />

    <Button
        android:id="@+id/btn_gyroscope"
        android:layout_width="117dp"
        android:layout_height="135dp"
        android:layout_marginStart="64dp"
        android:layout_marginTop="84dp"
        android:onClick="GyroscopeActivity"
        android:text="Wyslij dane z Å¼yroskopu na serwer"
        app:layout_constraintEnd_toStartOf="@+id/et_okres"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>